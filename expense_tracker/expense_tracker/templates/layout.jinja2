<!DOCTYPE html>
<html>
<head>
  <title>{% block title %}Expense App{% endblock %}</title>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script><script type="text/javascript"></script>
  <script src="https://use.fontawesome.com/4d0cb8d848.js"></script>
  <link rel="stylesheet" href="{{ request.static_url('expense_tracker:static/style.css') }}" type="text/css">
</head>
<body>
  <nav>
    <ul>
      <li><a href="{{ request.route_url('list') }}">Expenses</a></li>
      {% if request.authenticated_userid %}
        <li><a href="{{ request.route_url('create') }}">New Expense</a></li>
        <li><a href="{{ request.route_url('logout') }}">Log Out</a></li>
      {% else %}
        <li><a href="{{ request.route_url('login') }}">Log In</a></li>
      {% endif %}
    </ul>
  </nav>
  {% block body %}
  {% endblock %}
</body>
</html>